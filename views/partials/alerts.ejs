<% if(alerts.error) { %>
  <% alerts.error.forEach(function(message){ %>
    <div class="alert alert-danger">
      <%= message %>
    </div>
  <% }); %>
<% } %>
<% if(alerts.success) { %>
  <% alerts.success.forEach(function(message){ %>
    <div class="alert alert-success">
      <%= message %>
    </div>
  <% }); %>
<% } %>
